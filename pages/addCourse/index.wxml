<view class="container">
<view class="login-from">
  <picker class="pick-bg" bindchange="bindPickerChange" range="{{array}}">
    <view class="picker">
      星期：{{xingqi}}
    </view>
  </picker>
  <picker class="pick-bg" bindchange="bindPickerChange2" range="{{array2}}">
    <view class="picker">
      第几节：{{jieke}}
    </view>
  </picker>
  <picker class="pick-bg" bindchange="bindPickerChange3" range="{{array3}}">
    <view class="picker">
      开始周：{{zhou1}}
    </view>
  </picker>
  <picker class="pick-bg" bindchange="bindPickerChange4" range="{{array3}}">
    <view class="picker">
      结束周：{{zhou2}}
    </view>
  </picker>
  <view class="inputView">
    <label class="loginLab">课程</label>
    <input class="inputText" type="text" placeholder="请输入课程" bindinput="suojiaokechengInput"/>
  </view> 
  <view class="inputView">
    <label class="loginLab">授课教室</label>
    <input class="inputText" type="text" placeholder="请输入授课教室" bindinput="suoshoujiaoshiInput" />
  </view> 
  <view class="inputView">
    <label class="loginLab">授课老师</label>
    <input class="inputText" type="text" bindinput="jiaoshixingmingInput"/>
  </view> 

  <view class="line"></view>

  <!--按钮-->
  <view class="loginBtnView">
    <button class="loginBtn" type="primary" size="{{primarySize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login">提交</button>
  </view>
</view>
</view>